# archlinux: color ls by default
alias ls='ls --color=auto'

# make bash navigation easier
alias ..="cd .."

# videodownloader (for vimeo videos)
alias vimeo-dl='videodownloader -p Vimeo'

# vim shortcut
alias v='vim -p'

alias setdark='~/.local/opt/gnome-terminal-colors-solarized/set_dark.sh'
alias setlight='~/.local/opt/gnome-terminal-colors-solarized/set_light.sh'
